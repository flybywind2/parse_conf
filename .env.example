# ===== RAG 검색 =====
RAG_RETRIEVE_URL=http://apigw.server.net:8000/v2/retrieve-rrf
RAG_INDEX_NAME=rp-rag
RAG_API_KEY=__YOUR_RAG_API_KEY__
PASS_KEY=__YOUR_PASS_KEY__
NUM_RESULT_DOC=5
FIELDS_EXCLUDE=v_merge_title_content
RETRIEVE_FILTER_JSON=
VERIFY_SSL=true

# ===== RAG 업로드 =====
RAG_INSERT_URL=http://apigw.server.net:8000/v2/insert-doc
DEFAULT_PERMISSION_GROUPS=ds
USE_CHUNK_FACTOR=true
CHUNK_SIZE=200
CHUNK_OVERLAP=50
CHUNK_SEPARATOR= 

# ===== OpenAI 호환 게이트웨이 / LLM =====
OPENAI_BASE_URL=http://apigw.server.net/openai/v1
OPENAI_API_KEY=__YOUR_OPENAI_OR_PROXY_KEY__
X_DEP_TICKET=
SYSTEM_NAME=System-Name.
USER_ID=ID
USER_TYPE=Type
OPENAI_CHAT_MODEL=gpt-4o-mini
OPENAI_TEMPERATURE=0.2
QUESTION=이 문서들의 핵심 요지는 무엇인가?

# ===== Confluence 수집 =====
CONF_BASE_URL=https://confluence.example.com
CONF_USER=you@example.com
CONF_TOKEN=__YOUR_CONFLUENCE_TOKEN__
CONF_PAGE_IDS=12345,67890
CONF_RECURSIVE=true
CONF_MAX_DEPTH=3

# ===== 비전(이미지 설명) =====
VISION_ENABLE=false
VISION_MODEL=gpt-4o-mini
VISION_TEMPERATURE=0.2
VISION_MAX_IMAGES_PER_PAGE=8
VISION_TIMEOUT=90

# 이미지 축소 파라미터(게이트웨이 제한 회피)
VISION_MAX_BYTES_BEFORE=3000000
VISION_TARGET_MAX_BYTES=1200000
VISION_MAX_EDGE=1600
VISION_RETRIES=2
VISION_RETRY_WAIT=1.5
